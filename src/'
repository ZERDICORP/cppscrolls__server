package actions;
 
 
 
import zer.sql.SQLAction;



public class Action_ToggleBadReputation extends SQLAction
{
  {
    super.query("UPDATE unique_scroll_visits SET bad_mark = NOT bad_mark WHERE scroll_id = ? AND user_id = ?");
  }
  
  public Action_ToggleBadReputation(String scroll_id, String user_id)
  {
		put(scroll_id);
    put(user_id);
  }
}
